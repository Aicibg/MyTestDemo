<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dhao.mytestdemo">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_CONTACTS" />

    <application
        android:name=".dagger2.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".wallet.WalletAnimationActivity" />
        <activity android:name=".permission.PermissionActivity" />
        <activity android:name=".dagger2.DaggerActivity" />
        <activity android:name=".eventbus.EventbusActivity" />
        <activity android:name=".behavior.BehaviorActivity" />
        <activity android:name=".iconfont.IconFontActivity" />
        <activity android:name=".constraintlayout.ConstraintActivity" />
        <activity android:name=".mvp.LoadingMvpActivity" />
        <activity android:name=".mvp.LoginActivity" />
        <activity android:name=".apt.AptActivity"></activity>
    </application>

</manifest>